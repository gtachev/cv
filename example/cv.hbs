	<script type="text/javascript">
		'use strict';

		// Timeline init
		var holder = document.getElementById('svg-holder');
		var places = [
			{ id: 'education', name: 'Education', enabled: true, items: cv.data.getItemsData(document, 'div[data-page-name="education"]>div.timeline_item') },
			{ id: 'work', name: 'Work', enabled: true, items: cv.data.getItemsData(document, 'div[data-page-name="work"]>div.timeline_item') },
			{ id: 'projects', name: 'Projects', enabled: false, items: cv.data.getItemsData(document, 'div[data-page-name="projects"]>div.timeline_item') }
		];
		var defaultSkills = ['Go', 'Linux', 'DevOps', 'Security','Management', 'Data Wrangling', 'Docker', 'Rust', 'Bash', 'JavaScript', 'PHP', 'Machine Learning', 'Hiring', 'Teaching'];
		var chart;

		chart = new cv.MutiTimeline(holder, places, defaultSkills);
		['resize', 'hashchange', 'load'].forEach(function (e) {
			window.addEventListener(e, chart.xResizeDelayed.bind(chart));
		});

		window.matchMedia('print').addListener(function(mql) {
			if (mql.matches) {
				// Before print
				//window.location.hash = "";
				//holder.getElementsByTagName("svg").width = "750";
				chart.xResize();
			} else {
				// After print?
			}
		});

		// Handle print button
		document.querySelectorAll('a[data-print="true"]').forEach(function (el) {
			el.addEventListener('click', function (e) {
				e.preventDefault();
				window.print();
			});
		});

		document.querySelectorAll('a.place_link, a.project_link').forEach(function (el) {
			el.addEventListener('click', function (e) {
				setTimeout(function () {
					var sel = document.getElementById(el.getAttribute('data-item-id'));
					sel.scrollIntoView({behavior:'smooth', /*block: 'center',*/ inline: 'nearest'});
					sel.classList.add('highlighted');
					setTimeout(function () {
						sel.classList.remove('highlighted');
					}, 1000);
				}, 150);
			});
		});
	</script>
