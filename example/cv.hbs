<html lang="en">
<head>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">

	<title>{{personal.name}} - {{personal.title}}</title>
	<meta name="description" content="{{personal.short_bio}}" />

	<link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Open+Sans:300,400" >
	<link rel="stylesheet" href="../example/cv.css" media="screen">
	<link rel="stylesheet" href="../example/print.css" media="print">
	<script type="text/javascript" src="./bundle.js"></script>
	<noscript>
		<link rel="stylesheet" href="../example/noscript.css">
	</noscript>

	<link rel="icon" type="image/png" href="../example/favicon.png">
</head>


{{#*inline "skill"}}
	<div class="skill{{#if hideInList}} skill_list_hidden{{/if}}" data-strength="{{#if strength}}{{strength}}{{else}}1.0{{/if}}" {{#if from}} data-from="{{from}}" {{/if}} {{#if to}} data-to="{{to}}" {{/if}} {{#if description}} data-tooltip-toggle="true" {{/if}}>
		<span class="skill_name">{{name}}</span>
		{{#if description}}
			<div class="tooltip" data-position="bottom" data-color="dark">
				<div class="tooltip_content">{{{description}}}</div>
			</div>
		{{/if}}
	</div>
{{/inline}}

{{#*inline "item"}}
	<div class="timeline_item {{#each options}}{{this}}{{/each}}" data-type="work" id="{{id}}" data-label="{{label}}" {{#unless name }} style="display: none;" {{/unless}}>
		<div class="content_box timeline_content_box">
			<p class="timeline_date">
				<span class="fromdate">{{from}}</span>
				-
				<span class="todate">{{to}}</span>
			</p>
			<div class="timeline_title">
				{{#if place}}
					{{!-- This is a project --}}
					<h3 class="project_name"><a href="{{url}}" target="_blank">{{name}}</a></h3>
					{{#if place.name}}
						<h4>
								when <a class="place_link" href="#{{placeType}}" data-item-id="{{place.id}}">{{place.name}}</a>
								{{#if place.where}}
									at <a href="{{place.url}}" target="_blank">{{place.where}}</a>
								{{/if}}
							</a>
						</h4>
					{{/if}}
				{{else}}
					{{!-- This is a work/education item --}}
					<h3 class="place_name">{{name}}</h3>
					{{#if where}}
						<h4>
							at
							<a href="{{url}}" target="_blank"> {{where}}</a>
						</h4>
					{{/if}}
				{{/if}}
			</div>
			<div class="timeline_description">
				<p>{{{description}}}</p>
			</div>
			{{#each projects}}
				<div class="timeline_meta">
					<div class="timeline_skills">
						<a class="project_link" data-item-id="{{id}}" href="#projects">{{name}}</a>
						{{#each skills}} {{> skill }} {{/each}}
					</div>
				</div>
			{{/each}}
			{{#if skills}}
				<div class="timeline_meta">
					<div class="timeline_skills own_skills">
						{{#each skills}} {{> skill }} {{/each}}
					</div>
				</div>
			{{/if}}
		</div>
	</div>
	<!-- .timeline_item -->
{{/inline}}

<body>

	<div id="wrapper">

		<!-- CONTENT -->
		<div id="content_wrapper">
			<!-- WORK -->
			<span class="page_anchor" id="work"></span>
			<div class="single_page_content" data-page-id="3" data-page-name="work">
				<h1>Work</h1>
				{{#each work}} {{> item }} {{/each}}
			</div>
