    },
    "repository": "https://github.com/na--/d3-cv-multitimeline.git",
    "devDependencies": {
        "babel-core": "^6.26.0",
        "babel-plugin-external-helpers": "^6.22.0",
        "babel-preset-env": "^1.6.1",
        "eslint": "^4.11.0",
        "eslint-plugin-html": "^4.0.0",
        "eslint-plugin-import": "^2.8.0",
        "handlebars": "^4.0.11",
        "html-minifier": "^3.5.6",
        "inline-source": "^5.2.6",
        "rollup": "^0.51.8",
        "rollup-plugin-babel": "^3.0.2",
        "rollup-plugin-license": "^0.5.0",
