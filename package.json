{
    "name": "d3-multitimeline",
    "version": "0.1.0",
    "description": "An experiment with D3.js for visualizing a person's previous experience and skills via timelines",
    "keywords": [
        "d3",
        "cv",
        "skills",
        "timelines",
        "swimlanes",
        "stacked"
    ],
    "main": "scr/main.js",
    "scripts": {
        "watch": "watch-run -i -p '.*,*,src/**,example/**' 'yarn run build'",
        "build": "rm -rf build && mkdir build && rollup -c"
    },
    "author": {
        "name": "na--",
        "url": "https://github.com/na--"
    },
    "repository": "https://github.com/na--/cv-multitimeline.git",
    "devDependencies": {
        "babel-core": "^6.26.0",
        "babel-plugin-external-helpers": "^6.22.0",
        "babel-preset-env": "^1.6.1",
        "eslint": "^4.11.0",
        "eslint-plugin-html": "^4.0.0",
        "eslint-plugin-import": "^2.8.0",
        "inline-source": "^5.2.6",
        "rollup": "^0.51.8",
        "rollup-plugin-babel": "^3.0.2",
        "rollup-plugin-node-resolve": "^3.0.0"
    },
    "license": "GPL-3.0",
    "dependencies": {
        "d3-axis": "^1.0.8",
        "d3-scale": "^1.0.7",
        "d3-selection": "^1.2.0",
        "d3-selection-multi": "^1.0.1",
        "d3-time": "^1.0.8",
        "d3-time-format": "^2.1.1",
        "d3-transition": "^1.1.1",
        "html-minifier": "^3.5.6",
        "watch-run": "^1.2.5"
    }
}
